{
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "func-names": 0,
    "comma-dangle": 0,
    "arrow-parens": 0,
    "global-require": 0,
    "no-param-reassign": 0,
    "consistent-return": 0,
    "import/no-unresolved": 0,
    "space-before-function-paren": 0,
    "import/prefer-default-export": 1,
    // 开发过程中部分图片url路径过长，暂时关闭
    "max-len": [1, 170, 2, { ignoreComments: true, ignoreUrls: true }],
    "no-console": 0,

    "react/no-multi-comp": 0,
    "react/self-closing-comp": 0,
    "react/forbid-prop-types": 0,
    "react/no-array-index-key": 0,
    "react/no-unused-prop-types": 1,
    "react/no-render-return-value": 0,
    "linebreak-style": 0,
    "jsx-a11y/img-has-alt": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "react/prop-types": 0,
    "prefer-template": 0,
    "arrow-body-style": 0,
  }
}
